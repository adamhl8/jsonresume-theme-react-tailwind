{
  "name": "jsonresume-theme-react-tailwind",
  "version": "1.3.0",
  "type": "module",
  "description": "A JSON Resume theme built with React and Tailwind.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamhl8/jsonresume-theme-react-tailwind.git"
  },
  "homepage": "https://github.com/adamhl8/jsonresume-theme-react-tailwind",
  "bugs": {
    "url": "https://github.com/adamhl8/jsonresume-theme-react-tailwind/issues"
  },
  "author": {
    "email": "adamhl@pm.me",
    "name": "Adam Langbert",
    "url": "https://github.com/adamhl8"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist/"],
  "keywords": ["jsonresume", "jsonresume-theme"],
  "scripts": {
    "bundle": "bun lint && tsdown",
    "lint": "markdown-toc -i --bullets '-' --maxdepth 3 ./README.md && ts-import-fix -w && tsc --noEmit && biome check --write && adamhl8-knip",
    "prepare": "find .githooks -type f -exec ln -srf {} .git/hooks/ \\; || true",
    "prepublishOnly": "bun bundle",
    "refresh-schema": "bun run refresh-schema.ts",
    "render": "./scripts/render.sh",
    "watch": "./scripts/watch.sh"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.35.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.15"
  },
  "devDependencies": {
    "@adamhl8/configs": "^0.17.7",
    "@arethetypeswrong/core": "^0.18.2",
    "@biomejs/biome": "^2.2.7",
    "@tailwindcss/cli": "^4.1.15",
    "@types/bun": "latest",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "json-schema-to-typescript": "^15.0.4",
    "knip": "^5.66.2",
    "markdown-toc": "^1.2.0",
    "publint": "^0.3.15",
    "resumed": "^6.1.0",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3"
  }
}
